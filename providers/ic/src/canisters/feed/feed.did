type Time = int;
type FeedFilter = record {status: opt BlogPostStatus;};
type Feed = 
 service {
   accept: (principal, text) -> ();
   decline: (principal, text) -> ();
   list: (opt FeedFilter) -> (vec record {
                                    text;
                                    BlogPost;
                                  }) query;
   submit: (text, BlogPostSubmission) -> ();
 };
type BlogPostSubmission = 
 record {
   id: text;
   meta: blob;
   storageId: principal;
 };
type BlogPostStatus__1 = 
 variant {
   accepted;
   declined;
   open;
 };
type BlogPostStatus = 
 variant {
   accepted;
   declined;
   open;
 };
type BlogPost = 
 record {
   created_at: Time;
   id: text;
   meta: blob;
   status: BlogPostStatus__1;
   storageId: principal;
   updated_at: Time;
 };
service : (text) -> Feed
