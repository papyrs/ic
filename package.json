{
  "name": "ic",
  "version": "0.0.12",
  "author": "David Dal Busco",
  "description": "Backend canisters and providers of Papyrs",
  "private": true,
  "scripts": {
    "ic:admin": "node ./scripts/ic.admin.mjs",
    "ic:install": "node ./scripts/ic.installcode.mjs",
    "ic:transfer": "node ./scripts/ic.transfer.mjs",
    "ic:types": "node ./scripts/ic.types.js",
    "build": "npm run ic:types && cd providers/ic && npm run build",
    "postbuild": "npm run format",
    "format": "prettier --write providers/ic"
  },
  "workspaces": [
    "providers/*"
  ],
  "devDependencies": {
    "@stencil/core": "^2.15.2",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^27.5.1",
    "@types/puppeteer": "5.4.6",
    "autoprefixer": "^10.4.7",
    "jest": "^28.1.0",
    "jest-cli": "^28.1.0",
    "node-fetch": "^3.2.4",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "puppeteer": "^14.1.1",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/papyrs/ic.git"
  }
}
